{
  "name": "Shinyaigeek",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon dist/server.js --watch dist --watch server.js",
    "build:server": "webpack --config webpack.config.server.js --mode=development",
    "dev:client": "webpack-dev-server --config webpack.config.client.js --mode=development",
    "build:client": "webpack --config webpack.config.client.js --mode=production",
    "build:style:layout": "node-sass -r src/styles/layout.scss dist/layout.css --output-style compressed",
    "build:style:home": "node-sass -r src/styles/home.scss dist/home.css --output-style compressed",
    "build:style:profile": "node-sass -r src/styles/profile.scss dist/profile.css --output-style compressed",
    "build:style:post": "node-sass -r src/styles/post.scss dist/post.css --output-style compressed",
    "build:style": "yarn run build:style:layout && yarn run build:style:home && yarn run build:style:post && yarn run build:style:profile",
    "build": "webpack --config webpack.config.server.js --mode=none && yarn run build:style && yarn run build:client",
    "start": "ts-node server.ts",
    "perf": "ts-node scripts/perf.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shinyaigeek/Shinyaigeek.git"
