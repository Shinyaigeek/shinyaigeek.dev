{
  "name": "Shinyaigeek",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon dist/express.js --watch dist --watch express.js",
    "build:server": "webpack --config webpack.config.server.js --mode=development",
    "dev:client": "webpack-dev-server --config webpack.config.client.js --mode=development",
    "build:client": "webpack --config webpack.config.client.js --mode=development",
    "build:style:layout": "node-sass -r src/styles/layout.scss public/layout.css --output-style compressed",
    "build:style:home": "node-sass -r src/styles/home.scss public/home.css --output-style compressed",
    "build:style:profile": "node-sass -r src/styles/profile.scss public/profile.css --output-style compressed",
    "build:style:post": "node-sass -r src/styles/post.scss public/post.css --output-style compressed",
    "build:style": "yarn run build:style:layout && yarn run build:style:home && yarn run build:style:post && yarn run build:style:profile",
    "build": "webpack --config webpack.config.server.js --mode=production && webpack --config webpack.config.client.js --mode=production && yarn run build:style",
    "start": "node dist/express.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shinyaigeek/Shinyaigeek.git"
  },
  "keywords": [],
