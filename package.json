  },
  "homepage": "https://github.com/Shinyaigeek/Shinyaigeek#readme",
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@babel/core": "^7.8.7",
    "@types/express": "^4.17.2",
    "@types/highlight.js": "^9.12.3",
    "@types/marked": "^0.7.2",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "contentful": "^7.14.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.4.1",
    "fs": "^0.0.1-security",
    "highlight.js": "^9.18.1",
    "html-react-parser": "^0.10.1",
    "marked": "^0.8.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.2",
