{
  "name": "shinyaigeek.dev",
  "version": "1.0.0",
  "description": "",
  "main": "worker/dist/worker.js",
  "scripts": {
    "contenthash": "ts-node -O '{\"module\":\"commonjs\"}' shinyaigeek.dev/scripts/getContenthash.ts",
    "clear": "ts-node -O '{\"module\":\"commonjs\"}' shinyaigeek.dev/scripts/deleteDistAssets.ts",
    "rss": "ts-node -O '{\"module\":\"commonjs\"}' shinyaigeek.dev/scripts/buildRss.ts",
    "sitemap": "ts-node -O '{\"module\":\"commonjs\"}' shinyaigeek.dev/scripts/buildSitemap.ts",
    "purge": "ts-node -O '{\"module\":\"commonjs\"}' shinyaigeek.dev/scripts/purgeCache.ts",
    "mv:assets": "ts-node -O '{\"module\":\"commonjs\"}' shinyaigeek.dev/scripts/copyAssetsIntoPublic.ts",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "build:ssg": "yarn build:server && node server/main.js",
    "build:css": "webpack --config webpack.config.css.js --mode=production",
    "build": "NODE_ENV=production npm run clear && NODE_ENV=production npm run build:css && npm run contenthash && npm run rss && npm run sitemap && yarn mv:assets",
    "build:dev": "export NODE_ENV=development && npm run build:css && npm run build:ssg && npm run rss && npm run sitemap && yarn mv:assets",
    "build:server": "webpack --config webpack.config.server.js",
    "dev": "nodemon --watch src/ --exec  npm run build",
    "serve": "miniserve ./public --index index.html",
    "worker:publish": "wrangler publish ",
    "worker:build": "webpack --config webpack.config.worker.js",
    "d": "ts-node -O '{\"module\":\"commonjs\", \"jsx\":\"react\"}' shinyaigeek.dev/scripts/buildOGImage.tsx"
  },
