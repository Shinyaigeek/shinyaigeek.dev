{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "contenthash": "ts-node -O '{\"module\":\"commonjs\"}' scripts/getContenthash.ts",
    "clear": "ts-node -O '{\"module\":\"commonjs\"}' scripts/deleteDistAssets.ts",
    "rss": "ts-node -O '{\"module\":\"commonjs\"}' scripts/buildRss.ts",
    "upload": "ts-node -O '{\"module\":\"commonjs\"}' scripts/uploadAssetsToGCS.ts",
    "purge": "ts-node -O '{\"module\":\"commonjs\"}' scripts/purgeCache.ts",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "_build:ssg": "babel-node src/build/build.ts --config-file ./src/build/babel.config.js --extensions '.ts,.tsx'",
    "_build:css": "webpack --config webpack.config.css.js --mode=production",
    "_build": "export NODE_ENV=production && npm run clear && npm run _build:css && npm run contenthash && npm run rss",
    "_build:dev": "export NODE_ENV=development && npm run clear && npm run _build:css && npm run _build:ssg && npm run rss",
    "dev": "nodemon --watch src/ --exec  npm run _build",
    "serve": "miniserve dist --index index.html"
  },
