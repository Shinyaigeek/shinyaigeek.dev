{
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "/dist/*.js",
            "use": "@now/static"
        },
        {
            "src": "/public/*.css",
            "use": "@now/static"
        },
        {
            "src": "/static/*.png",
            "use": "@now/static"
        },
        {
            "src": "/dist/server/server.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/profile.js",
            "dest": "dist/profile.js"
        },
        {
            "src": "/home.css",
            "dest": "dist/home.css"
        },
        {
            "src": "/layout.css",
            "dest": "dist/home.css"
        },
        {
            "src": "/profile.css",
            "dest": "dist/home.css"
        },
        {
            "src": "/post.css",
            "dest": "dist/home.css"
        },
        {
            "src": "/banana.png",
            "dest": "static/banana.png"
        },
        {
            "src": "/earth.png",
            "dest": "static/earth.png"
        },
        {
            "src": "/icon_blur.png",
            "dest": "static/icon_blur.png"
        },
        {
            "src": "/icon_thatsme.png",
            "dest": "static/icon_thatsme.png"
        },
        {
            "src": "/icon_transparent_header.png",
            "dest": "static/icon_transparent_header.png"
        },
        {
            "src": "/icon_transparent.png",
            "dest": "static/icon_transparent.png"
        },
        {
            "src": "/icon.png",
            "dest": "static/icon.png"
        },
        {
            "src": "/.*",
            "dest": "dist/server/server.js"
        }
    ],
    "env": {
        "CONTENTFUL_SPACE_ID": "@contentful-space-id-production-4lm2",
        "CONTENTFUL_ACCESS_TOKEN": "@contentful-access-token-production-ra4w"
    }
}