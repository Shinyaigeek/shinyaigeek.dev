{
    "routes": [
        {
            "src": "/style.js",
            "dest": "dist/profile.js"
        },
        {
            "src": "/banana.png",
            "dest": "static/banana.png"
        },
        {
            "src": "/earth.png",
            "dest": "static/earth.png"
        },
        {
            "src": "/icon_blur.png",
            "dest": "static/icon_blur.png"
        },
        {
            "src": "/icon_thatsme.png",
            "dest": "static/icon_thatsme.png"
        },
        {
            "src": "/icon_transparent_header.png",
            "dest": "static/icon_transparent_header.png"
        },
        {
            "src": "/icon_transparent.png",
            "dest": "static/icon_transparent.png"
        },
        {
            "src": "/icon.png",
            "dest": "static/icon.png"
        },
        {
            "src": "/.*",
            "dest": "api/server/server.js"
        }
    ],
    "functions": {
        "dist/server/server.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "env": {
        "CONTENTFUL_SPACE_ID": "@contentful-space-id-production-4lm2",
        "CONTENTFUL_ACCESS_TOKEN": "@contentful-access-token-production-ra4w"
    }
}